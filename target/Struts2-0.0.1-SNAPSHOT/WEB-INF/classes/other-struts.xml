<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
  "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
  "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
  <package name="other" namespace="/other" extends="default">

    <!-- String -->
    <action name="string" class="com.action.other.JavabaseAction" method="string">
      <result name="string">/WEB-INF/views/other/javabaseSuccess.jsp</result>
    </action>
    
    <!-- Type change -->
    <action name="typeChange" class="com.action.other.JavabaseAction" method="typeChange">
      <result name="type_change">/WEB-INF/views/other/javabaseSuccess.jsp</result>
    </action>
    
    <!-- Date -->
    <action name="dateSample" class="com.action.other.JavabaseAction" method="dateSample">
      <result name="date_sample">/WEB-INF/views/other/javabaseSuccess.jsp</result>
    </action>
    
    <!-- Map -->
    <action name="mapSample1" class="com.action.other.JavabaseAction" method="mapSample1">
      <result name="map_sample1">/WEB-INF/views/other/javabaseSuccess.jsp</result>
    </action>
    <action name="mapSample2" class="com.action.other.JavabaseAction" method="mapSample2">
      <result name="map_sample2">/WEB-INF/views/other/javabaseSuccess.jsp</result>
    </action>
    
    <!-- List -->
    <action name="listSample1" class="com.action.other.JavabaseAction" method="listSample1">
      <result name="list_sample1">/WEB-INF/views/other/javabaseSuccess.jsp</result>
    </action>
    
    <!-- No UI Tag -->
    <action name="nonui" class="com.action.other.StrutsTagAction" method="nonUi">
      <result name="nonui_success">/WEB-INF/views/other/nonuiSuccess.jsp</result>
    </action>
  </package>
</struts>